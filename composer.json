{
    "name": "patgod85/phpdoc2rst",
    "type": "symfony-bundle",
    "description": "PHPDoc to RST",
    "homepage": "https://github.com/patgod85/phpdoc2rst",
    "keywords": ["documentation", "phpdoc", "rst"],
    "license" : "MIT",
    "authors": [
        {
            "name":     "Victor",
            "homepage": "https://github.com/patgod85",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2",
        "andrewsville/php-token-reflection": "dev-master",
        "doctrine/annotations": "*",
        "doctrine/inflector": "*",
        "jms/serializer": "^1.4",
        "symfony/config": ">2.5",
        "symfony/console": ">=2.0.10",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": ">2.5",
        "symfony/dependency-injection": ">2.5",
        "symfony/process": ">=2.0.10",
        "twig/twig": "1.*"
    },
    "autoload": {
        "psr-0": { "Patgod85\\Phpdoc2rst": "" }
    },
    "autoload-dev": {
        "psr-0": { "input": "./Tests" }
    },
    "target-dir" : "Patgod85/Phpdoc2rst",
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": ">=1.6"
    }
}
