{
    "name": "patgod85/phpdoc2rst",
    "version": "0.0.2",
    "type": "library",
    "description": "PHPDoc to RST",
    "keywords": ["documentation", "phpdoc", "rst", "sphinx"],
    "authors": [
        {
            "name":     "Victor",
            "email":    "victor@realrussia.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2",
        "symfony/console": ">=2.0.10",
        "symfony/process": ">=2.0.10",
        "andrewsville/php-token-reflection": "dev-master",
        "phpunit/phpunit": ">=1.6",
        "doctrine/inflector": "*",
        "doctrine/annotations": "*",
        "twig/twig": "1.*"
    },
    "bin": ["bin/phpdoc2rst.bat", "bin/phpdoc2rst-trainsEmail.bat", "bin/phpdoc2rst-trainsGateway.bat", "bin/phpdoc2rst-trainsGateway-v2.bat"],
    "autoload": {
        "psr-0": { "Phpdoc2rst": "src/" }
    },

    "autoload-dev": {
        "psr-0": { "input": "src/Phpdoc2rst/Tests" }
    }

}
